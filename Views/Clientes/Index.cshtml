@model IEnumerable<Cliente>

@{
    //Usada para trocar o template padrao da pagina
    @* Layout = null; *@
}

<div class="container">
    <partial name="_SearchCliente">
    <div class="row">
        <div class="col-sm-10 col-md-10 col-lg-10">
            <table class="table table-dark table-striped text-center">
                <thead>
                    <tr>
                        <br>
                        <th>@Html.DisplayNameFor(Model => Model.Nome)</th>
                        <th>@Html.DisplayNameFor(Model => Model.Contato)</th>
                        <th>@Html.DisplayNameFor(Model => Model.Email)</th>
                        <th>@Html.DisplayNameFor(Model => Model.Cidade)</th>
                        <th>@Html.DisplayNameFor(Model => Model.Estado)</th>
                        <th><input type="hidden" name=""></th>
                        <th><input type="hidden" name=""></th>
                        <th><input type="hidden" name=""></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var i in Model)
                    {
                        <partial name="_ClienteResumo" for="@i" />
                    }
                </tbody>
            </table>
        </div>
    </div>


    @* <h1>@ViewData["Data"]</h1>
    <h2>@ViewBag.Total:@ViewBag.TotalClientes</h2>
    <h2>@TempData["Nome"]</h2> *@

</div>

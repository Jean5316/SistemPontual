@model ClienteListViewModel
@{
    //Usada para trocar o template padrao da pagina
    @* Layout = null; *@
}


<div class="container">
    <h1 class="Nome">@ViewBag.Titulo</h1>
    <a asp-action="Criar"><button class="btn btn-primary">Adicionar Cliente</button></a>
    <br>
    <br>
    <div class="row">
        <div class="col-sm-10 col-md-10 col-lg-10">
            <table class="table table-dark table-striped text-center">
                <thead>
                    <tr>
                        <br>
                        <th>Nome</th>
                        <th>Numero de Contato</th>
                        <th>Email</th>
                        <th>Cidade</th>
                        <th>Estado</th>
                        <th><input type="hidden" name=""></th>
                        <th><input type="hidden" name=""></th>
                        <th><input type="hidden" name=""></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var i in Model.Clientes)
                    {
                        <partial name="_ClienteResumo" for="@i" />
                    }
                </tbody>
            </table>
        </div>
    </div>


    @* <h1>@ViewData["Data"]</h1>
    <h2>@ViewBag.Total:@ViewBag.TotalClientes</h2>
    <h2>@TempData["Nome"]</h2> *@

</div>
